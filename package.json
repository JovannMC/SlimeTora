{
  "name": "slimetora",
  "productName": "slimetora",
  "version": "1.0.0-alpha1",
  "description": "A program that connects the HaritoraX Wireless trackers to the SlimeVR server",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "build-bulma": "npx sass --watch ./src/index.scss ./src/index.css",
    "rebuild": "electron-rebuild",
    "build": "electron-packager . SlimeTora --platform=win32 --arch=x64 --overwrite --out='dist/'"
  },
  "devDependencies": {
    "electron": "29.1.6",
    "electron-rebuild": "^3.2.9"
  },
  "keywords": [],
  "author": {
    "name": "JovannMC",
    "email": "jovannmc@femboyfurry.net"
  },
  "license": "MIT",
  "dependencies": {
    "bulma": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "haritorax-interpreter": "^1.4.4",
    "quaternion": "^1.5.1",
    "sass": "^1.72.0"
  }
}
